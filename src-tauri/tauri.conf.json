{
    
  "build": {
      "withGlobalTauri": true,
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../public",
    "distDir": "../public"
  },
  "package": {
    "productName": "Ternel",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
        "shell": {
            "all": true,
            "execute": true,
            "sidecar": true,
            "open": true,
"scope": [
        {
            "name": "Bun",
            "cmd": "Bun"
        }
    ]
        },
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "Sujauddin",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/favicon.png"
      ],
      "identifier": "com.ternel.app",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "width": 1600,
        "height": 900,
        "resizable": true,
        "title": "Ternel"
      }
    ]
  }
}
